#include<stdio.h>
#include<cs50.h>
int main()
{
    int num1 = get_int("Enter number 1:\n");
    int num2 = get_int("Enter second number:\n");

    int sum, diff, prod, quot, rem;

    sum = num1 + num2;
    diff= num1 - num2;
    prod = num1 * num2;
    quot = num1 / num2;
    rem = num1%num2;

    printf("The sum of two integer is %d\n", sum);
    printf("The different is %d\n", diff);
    printf("The product is %d\n", prod);
    printf("The remeainder is %d\n", rem);

    return 0;


}
